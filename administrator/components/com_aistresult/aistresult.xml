<?xml version="1.0"?>
<extension type="component" version="1.6.0" method="upgrade">
    <name>com_aistresult</name>
    <author>Arhet</author>
    <authorEmail>arhetstudio@gmail.com</authorEmail>
    <authorUrl></authorUrl>
    <copyright></copyright>
    <license>GNU</license>
    <version>1.0</version>
    <description>COM_AISTRESULT_DESC</description>
    <creationDate>13 April 2016</creationDate>
    <files folder="site">
        <filename>index.html</filename>
        <filename>aistresult.php</filename>
        <filename>controller.php</filename>
        <filename>router.php</filename>
        <folder>models</folder>
        <folder>views</folder>
        <folder>controllers</folder>
        <folder>assets</folder>
        <folder>helpers</folder>
    </files>
    <install>
        <sql>
            <file driver="mysql" charset="utf8">sql/install.mysql.utf8.sql</file>
        </sql>
    </install>
    <uninstall>
        <sql>
            <file driver="mysql" charset="utf8">sql/uninstall.mysql.utf8.sql</file>
        </sql>
    </uninstall>
    <languages folder="site">
        <language tag="en-GB">language/en-GB/en-GB.com_aistresult.ini</language>
        <language tag="ru-RU">language/ru-RU/ru-RU.com_aistresult.ini</language>
    </languages>
    <administration>
        <menu>COM_AISTRESULT</menu>
        <submenu>
			<menu link="option=com_aistresult&amp;view=graphss" view="graphss" img="class:graphss" alt="Aistresult/Graphs">GRAPHS_SUBMENU</menu>
        </submenu>
        <files folder="admin">
            <filename>index.html</filename>
            <filename>aistresult.php</filename>
            <filename>controller.php</filename>
            <filename>config.xml</filename>
            <filename>access.xml</filename>
            <folder>sql</folder>
            <folder>tables</folder>
            <folder>models</folder>
            <folder>views</folder>
            <folder>controllers</folder>
            <folder>helpers</folder>
        </files>
        <languages folder="admin">
            <language tag="en-GB">language/en-GB/en-GB.com_aistresult.ini</language>
            <language tag="ru-RU">language/ru-RU/ru-RU.com_aistresult.ini</language>
            <language tag="en-GB">language/en-GB/en-GB.com_aistresult.sys.ini</language>
            <language tag="ru-RU">language/ru-RU/ru-RU.com_aistresult.sys.ini</language>
        </languages>
    </administration>
</extension>